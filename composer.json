{
    "name": "daviddel/doctrine-manager",
    "type": "library",
    "description": "Manager Doctrine Objects",
    "keywords": [],
    "license": "MIT",
    "authors": [
        {
            "name": "David Delevoye",
            "email": "daviddelevoye@gmail.com"
        }
    ],
    "require": {
        "doctrine/common": ">=2.0",
        "symfony/dependency-injection": ">=2.2"
    },
    "require-dev": {
        "doctrine/orm": ">=2.0",
        "friendsofsymfony/elastica-bundle": "^3.1"
    },
    "suggest": {
        "doctrine/orm": "to use the extensions with the ORM",
        "friendsofsymfony/elastica-bundle": "to use the extensions with the Elastic"
    },
    "autoload": {
        "psr-0": { "Doctrine\\Manager\\": "lib/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
